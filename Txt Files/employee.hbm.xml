<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.entity.Employee" table="Employee100">
	
		<id name="employeeId" type="int" column="EmpId">
			<generator class="assigned"/>
		</id>

		<property name="employeeName" column="EmpName" />
	
		<property name="employeeAddress">
			<column name="EAddress" />
		</property>
		
			<property name="employeeSalary" column="Salary" type="int" />
	</class>
</hibernate-mapping>